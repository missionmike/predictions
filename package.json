{
  "name": "",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run format:check && npm run lint && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,astro,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,astro,css}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "test": "vitest"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.5",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.1",
    "astro": "^5.7.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@typescript-eslint/parser": "^8.31.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.25.1",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.87.0",
    "typescript-eslint": "^8.31.0",
    "vitest": "^3.1.2"
  }
}
